					background-color: rgb(0, 0, 0) /*bg_gen*/;
					width: 16px; 
					border-width: 1px;
				}
				QDoubleSpinBox::up-arrow,
				QSpinBox::up-arrow,
				QDateTimeEdit::up-arrow
				{
					image: url(:/16x16/icons/16x16/up.gif);
