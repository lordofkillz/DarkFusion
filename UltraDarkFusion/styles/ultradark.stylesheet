					selection-background-color: rgb(236, 157, 0) /*accent*/;
					outline: 0;
				}
				QComboBox::drop-down
				{
					background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0  rgb(0, 0, 0) /*bg_gen*/,stop:1 rgb(18, 18, 18) /*bg_prim*/);
					subcontrol-origin: padding;
