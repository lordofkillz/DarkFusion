				QWidget
				{
					background-color: rgb(18, 18, 18) /*bg_prim*/;
					color: rgb(238, 238, 238) /*txt*/;
					selection-background-color: rgb(56, 56, 56) /*bg_alt*/;
					selection-color: rgb(28, 28, 28) /*txt_bg*/;
					border-radius: 6px;
				}
				/*-----QLabel-----*/
				QLabel
				{
					background-color: transparent;
					color: rgb(238, 238, 238) /*txt*/;
				}
				/*-----QMenuBar-----*/
				QMenuBar 
				{
					background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgb(56, 56, 56) /*bg_alt*/, stop:1 rgb(18, 18, 18) /*bg_prim*/);
					border: 1px solid #000;
					color: rgb(238, 238, 238) /*txt*/;
					border-radius: 0px;
				}
				QMenuBar::item 
				{
					background-color: transparent;
				}
				QMenuBar::item:selected 
				{
					background-color: rgba(236, 157, 0, 51) /*accent_t*/;
					border: 1px solid rgb(236, 157, 0) /*accent*/;
					color: rgb(238, 238, 238) /*txt*/;
				}
				QMenuBar::item:pressed 
				{
					background-color: rgb(236, 157, 0) /*accent*/;
					border: 1px solid rgb(236, 157, 0) /*accent*/;
					color: rgb(238, 238, 238) /*txt*/;
				}
				/*-----QMenu-----*/
				QMenu
				{
					background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgb(18, 18, 18) /*bg_prim*/, stop:1 rgb(0, 0, 0) /*bg_gen*/);
					border: 1px solid #000;
					padding: 4px;
					color: rgb(238, 238, 238) /*txt*/;
				}
				QMenu::item
				{
					background-color: transparent;
					padding: 2px 20px 2px 20px;
				}
				QMenu::separator
				{
					background-color: rgb(236, 157, 0) /*accent*/;
					height: 1px;
				}
				QMenu::item:disabled
				{
					color: #555;
					background-color: transparent;
					padding: 2px 20px 2px 20px;
				}
				QMenu::item:selected
				{
					background-color: rgba(236, 157, 0, 51) /*accent_t*/;
					border: 1px solid rgb(236, 157, 0) /*accent*/;
					color: rgb(238, 238, 238) /*txt*/;
				}

				/*-----QPushButton-----*/
				QPushButton
				{
					background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgb(56, 56, 56) /*bg_alt*/,stop:1 rgb(0, 0, 0) /*bg_gen*/);
					color: rgb(238, 238, 238) /*txt*/;
					min-width: 80px;
					border-style: solid;
					border-width: 1px;
					border-radius: 4px;
					border-color: rgb(0, 0, 0) /*bg_gen*/;
					padding: 5px;
				}
				QPushButton::flat
				{
					background-color: transparent;
					border: none;
					color: rgb(238, 238, 238) /*txt*/;
				}
				QPushButton::disabled
				{
					background-color: #404040;
					color: #656565;
					border-color: rgb(0, 0, 0) /*bg_gen*/;
				}
				QPushButton::hover
				{
					background-color: rgba(236, 157, 0, 51) /*accent_t*/;
					border: 1px solid rgb(236, 157, 0) /*accent*/;
				}
				QPushButton::pressed
				{
					background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgb(18, 18, 18) /*bg_prim*/,stop:1 rgb(0, 0, 0) /*bg_gen*/);
					border: 1px solid rgb(236, 157, 0) /*accent*/;
				}
				QPushButton::checked
				{
					background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgb(18, 18, 18) /*bg_prim*/,stop:1 rgb(0, 0, 0) /*bg_gen*/);
					border: 1px solid #222;
				}
				/*-----QLineEdit-----*/
				QLineEdit
				{
					background-color: rgb(28, 28, 28) /*txt_bg*/;
					color : rgb(238, 238, 238) /*txt*/;
					border: 1px solid #222;
					border-radius: 2px;
					padding: 3px;
					padding-left: 5px;
				}
				/*-----QPlainTExtEdit-----*/
				QPlainTextEdit
				{
					background-color: rgb(28, 28, 28) /*txt_bg*/;
					color : rgb(238, 238, 238) /*txt*/;
					border: 1px solid #222;
					border-radius: 2px;
					padding: 3px;
					padding-left: 5px;
				}
				/*-----QTabBar-----*/
				QTabBar::tab
				{
					background-color: qlineargradient(spread:repeat, x1:1, y1:1, x2:1, y2:0, stop:0 rgb(18, 18, 18) /*bg_prim*/,stop:1 rgb(56, 56, 56) /*bg_alt*/);
					color: rgb(238, 238, 238) /*txt*/;
					border-style: solid;
					border-width: 1px;
					border-color: #444;
					border-bottom: none;
					min-width: 50px;
					padding: 3px;
					/*padding-top: 11px;*/
					/*padding-bottom: 11px;*/
					/*border-radius: 3px;*/
				}
				QTabWidget::pane 
				{
					border: 1px solid  rgba(255, 255, 255, 0);
					/*top: 1px;*/
					border-radius: 8px;
					/*background: rgba(255, 0, 255, 50);*/
				}
				QTabBar::tab:last
				{
					margin-right: 0; 
				}
				QTabBar::tab:first:!selected
				{
					/*background-color: rgb(18, 18, 18) bg_prim*/;
					margin-left: 0px;
				}
				QTabBar::tab:!selected
				{
					color: rgb(238, 238, 238) /*txt*/;
					border-bottom-style: solid;
					background-color: qlineargradient(spread:repeat, x1:1, y1:1, x2:1, y2:0, stop:0  rgb(0, 0, 0)/*bg_gen*/,stop:1 rgb(18, 18, 18) /*bg_prim*/);
				}
				QTabBar::tab:selected
				{
					margin-bottom: 0px;
				}
				QTabBar::tab:!selected:hover
				{
					
					background-color: rgba(236, 157, 0, 51) /*accent - accent_t*/;
				}

				/*-----QComboBox-----*/
				QComboBox
				{
					background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0  rgb(56, 56, 56) /*bg_alt*/,stop:1 rgb(18, 18, 18) /*bg_prim*/);
					border: 1px solid #000;
					padding-left: 6px;
					color: rgb(238, 238, 238) /*txt*/;
					height: 20px;
				}
				QComboBox::disabled
