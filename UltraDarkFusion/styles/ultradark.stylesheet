					background-color: #404040;
					color: #656565;
					border-color: rgb(0, 0, 0) /*bg_gen*/;
				}
				QComboBox:on
				{
					background-color: rgb(236, 157, 0) /*accent*/;
					color: rgb(28, 28, 28) /*txt_bg*/;
				}
				QComboBox QAbstractItemView
				{
					background-color: rgb(18, 18, 18) /*bg_prim*/;
