				QComboBox::down-arrow
				{
					image: url(:/16x16/icons/16x16/down.gif);
					width: 8px;
					height: 8px;
				}
				/*-----QSpinBox & QDateTimeEdit-----*/
				QSpinBox,QDoubleSpinBox,
