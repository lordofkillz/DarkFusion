				{
					background-color: rgb(0, 0, 0) /*bg_gen*/;
					color : rgb(238, 238, 238) /*txt*/;
					border: 1px solid #000;
					padding: 3px;
					padding-left: 5px;
					border-radius : 2px;
				}
				QDoubleSpinBox::up-button, 
				QSpinBox::up-button, 
