					height: 7px;
				}
				/*-----QGroupBox-----*/
				QGroupBox 
				{
					border: 1px solid;
					border-color: #666666;
					border-radius: 5px;
					margin-top: 20px;
				}
				QGroupBox::title  
				{
					background-color: transparent;
					color: rgb(238, 238, 238) /*txt*/;
					subcontrol-origin: margin;
					padding: 5px;
					border-top-left-radius: 3px;
					border-top-right-radius: 3px;
				}


				QCheckBox{
					background-color: transparent;
					color: rgb(238, 238, 238) /*txt*/;
					border: none;
				}
				QCheckBox::disabled
				{
					color: #656565;
				}
				/*-----QRadioButton-----*/
				QRadioButton 
				{
					color: lightgray;
					background-color: transparent;
				}
				QRadioButton::indicator::unchecked:hover 
				{
					background-color: lightgray;
					border: 2px solid rgb(236, 157, 0) /*accent*/;
					border-radius: 6px;
				}
				QRadioButton::indicator::checked 
				{
					border: 2px solid rgb(236, 157, 0) /*accent*/;
					border-radius: 6px;
					background-color: rgba(236, 157, 0, 51) /*accent_t*/;  
					width: 9px; 
					height: 9px; 
				}
				/*-----QSlider-----*/
				QSlider::groove:horizontal 
				{
					background-color: transparent;
					height: 6px;
					border: 1px solid #bbb;
					border-radius: 4px;
				}
				QSlider::sub-page:horizontal 
				{
					background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 rgb(236, 157, 0) /*accent*/, stop: 1 rgb(28, 28, 28) /*txt_bg*/);
					background: qlineargradient(x1: 0, y1: .9, x2: 1, y2: 1,stop: 0 rgb(28, 28, 28) /*txt_bg*/, stop: 1 rgb(236, 157, 0) /*accent*/);
					border: 1px solid #000;
					height: 6px;
					border-radius: 4px;
				}
				QSlider::add-page:horizontal 
				{
					background-color: rgb(28, 28, 28) /*txt_bg*/;
					border: 1px solid #000;
					height: 6px;
					border-radius: 4px;
				}
				QSlider::handle:horizontal 
				{
					background-color: rgb(236, 157, 0) /*accent*/;
					border: 1px solid #000;
					width: 18px;
					margin-top: -3px;
					margin-bottom: -3px;
					border-radius: 4px;
				}
				QSlider::handle:horizontal:hover 
				{
					background-color: #333;
					border: 1px solid #000;
					border-radius: 4px;
				}
				QSlider::sub-page:horizontal:disabled 
				{
					background-color: #bbb;
					border-color: #999;
				}
				QSlider::add-page:horizontal:disabled 
				{
					background-color: rgb(238, 238, 238) /*txt*/;
					border-color: #999;
				}
				QSlider::handle:horizontal:disabled 
				{
					background-color: rgb(238, 238, 238) /*txt*/;
					border: 1px solid #aaa;
					border-radius: 3px;
				}
				/*-----QSlider-----*/
				QSlider::groove:vertical 
				{
					background-color: transparent;
					width: 6px;
					border: 1px solid #bbb;
					border-radius: 4px;
				}
				QSlider::sub-page:vertical 
				{
					background-color: rgb(28, 28, 28) /*txt_bg*/;
					border: 1px solid #000;
					width: 6px;
					border-radius: 4px;
				}
				QSlider::add-page:vertical 
				{
					background: qlineargradient(x1: 1, y1: 1, x2: 1, y2: 1,stop: 0 rgb(236, 157, 0) /*accent*/, stop: 1 rgb(28, 28, 28) /*txt_bg*/);
					background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 1,stop: 0 rgb(28, 28, 28) /*txt_bg*/, stop: 1 rgb(236, 157, 0) /*accent*/);
					border: 1px solid #000;
					width: 6px;
					border-radius: 4px;
				}
				QSlider::handle:vertical 
				{
					background-color: rgb(236, 157, 0) /*accent*/;
					border: 1px solid #000;
					height: 18px;
					margin-left: -3px;
					margin-right: -3px;
					border-radius: 4px;
				}
				QSlider::handle:vertical:hover 
				{
					background-color: #333;
					border: 1px solid #000;
					border-radius: 4px;
				}
				QSlider::sub-page:vertical:disabled 
				{
					background-color: rgb(238, 238, 238) /*txt*/;
					border-color: #999;
				}
				QSlider::add-page:vertical:disabled 
				{
					background-color: #bbb;
					border-color: #999;
				}
				QSlider::handle:vertical:disabled 
				{
					background-color: rgb(238, 238, 238) /*txt*/;
					border: 1px solid #aaa;
					border-radius: 3px;
				}
				/*-----QScrollBar-----*/
				QScrollBar:horizontal
				{
					border: 1px solid #777;
					background-color: rgb(18, 18, 18) /*bg_prim*/;
					height: 15px;
					margin: 0px 16px 0 16px;
				}
				QScrollBar::handle:horizontal
				{
					background-color: qlineargradient(spread:repeat, x1:0, y1:0, x2:0, y2:0, stop:0 rgb(56, 56, 56) /*bg_alt*/,stop:1 rgb(18, 18, 18) /*bg_prim*/);
					border: 1px solid #000;
					min-height: 20px;
				}
				QScrollBar::add-line:horizontal
				{
					background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgb(56, 56, 56) /*bg_alt*/,stop:1 rgb(18, 18, 18) /*bg_prim*/);
					border: 1px solid #777;
					width: 15px;
					subcontrol-position: right;
					subcontrol-origin: margin;
				}
				QScrollBar::sub-line:horizontal
				{
					background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgb(56, 56, 56) /*bg_alt*/,stop:1 rgb(18, 18, 18) /*bg_prim*/);
					border: 1px solid #777;
					width: 15px;
					subcontrol-position: left;
					subcontrol-origin: margin;
				}
				QScrollBar::right-arrow:horizontal
				{
					image: url(:/16x16/icons/16x16/right.gif);
					width: 6px;
					height: 6px;
				}
				QScrollBar::left-arrow:horizontal
				{
					image: url(:/16x16/icons/16x16/left.gif);
					width: 6px;
					height: 6px;
				}
				QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
				{
					background: none;
				}
				QScrollBar:vertical
				{
					background-color: rgb(18, 18, 18) /*bg_prim*/;
					width: 16px;
					border: 1px solid #777;
					margin: 16px 0px 16px 0px;
				}
				QScrollBar::handle:vertical
				{
					background-color: qlineargradient(spread:repeat, x1:0, y1:0, x2:0, y2:0, stop:0 rgb(56, 56, 56) /*bg_alt*/,stop:1 rgb(18, 18, 18) /*bg_prim*/);
					border: 1px solid #000;
					min-height: 20px;
				}
				QScrollBar::add-line:vertical
				{
					background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgb(56, 56, 56) /*bg_alt*/,stop:1 rgb(18, 18, 18) /*bg_prim*/);
					border: 1px solid #777;
					height: 15px;
					subcontrol-position: bottom;
					subcontrol-origin: margin;
				}
				QScrollBar::sub-line:vertical
				{
					background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgb(56, 56, 56) /*bg_alt*/,stop:1 rgb(18, 18, 18) /*bg_prim*/);
					border: 1px solid #777;
					height: 15px;
					subcontrol-position: top;
					subcontrol-origin: margin;
