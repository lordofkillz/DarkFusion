				{
					background-color: rgb(56, 56, 56) /*bg_alt*/;
				}
				QDoubleSpinBox::down-button:pressed,
				QSpinBox::down-button:pressed, 
				QDateTimeEdit::down-button:pressed
				{
					background-color: rgb(0, 0, 0) /*bg_gen*/;
					width: 16px; 
					border-width: 1px;
				}
				QDoubleSpinBox::down-arrow,
				QSpinBox::down-arrow,
				QDateTimeEdit::down-arrow
				{
					image: url(:/16x16/icons/16x16/down.gif);
